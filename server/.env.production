PORT=5000
NODE_ENV=production
MONGODB_URI=${MONGODB_URI}
JWT_SECRET=${JWT_SECRET}
API_URL=https://msd-project-8c1o.onrender.com
CLIENT_URL=https://kranthi-project-deployer.vercel.app

# OAuth Configuration
# These should be set in Render environment variables, not hardcoded here
GOOGLE_CLIENT_ID=${GOOGLE_CLIENT_ID}
GOOGLE_CLIENT_SECRET=${GOOGLE_CLIENT_SECRET}

# Get these from GitHub (https://github.com/settings/developers)
GITHUB_CLIENT_ID=${GITHUB_CLIENT_ID}
GITHUB_CLIENT_SECRET=${GITHUB_CLIENT_SECRET}

# Provider Integrations
# Vercel (get from https://vercel.com/account/tokens)
VERCEL_TOKEN=${VERCEL_TOKEN}
VERCEL_TEAM_ID=${VERCEL_TEAM_ID}

# Netlify (get from https://app.netlify.com/user/applications)
NETLIFY_TOKEN=${NETLIFY_TOKEN}
NETLIFY_WEBHOOK_SECRET=${NETLIFY_WEBHOOK_SECRET}

# Render (get from https://dashboard.render.com/account/api-tokens)
RENDER_API_KEY=${RENDER_API_KEY}
RENDER_WEBHOOK_SECRET=${RENDER_WEBHOOK_SECRET}

# Webhook Secrets for signature validation
VERCEL_WEBHOOK_SECRET=${VERCEL_WEBHOOK_SECRET}
GITHUB_WEBHOOK_SECRET=${GITHUB_WEBHOOK_SECRET}

